class Solution:
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        if needle=='':
            return 0
        l=len(needle)
        for i in range(len(haystack)):
            if haystack[i]==needle[0]:
                if haystack[i:i+l]==needle:
                    return i
        return -1
