class Solution:
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        s1=s.lower()
        print(s1)
        L=[]
        for i in range(len(s)):
            if (s1[i]<='z' and s1[i]>='a') or (s1[i]>='0' and s1[i]<='9'):
                L.append(s1[i])
        print(L)
        if L==L[::-1]:
            return True
        return False
